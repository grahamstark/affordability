@@INCLUDE@@ ../components/oscr_header.thtml "Output Breakdown : @_REGIME_NAME_@ : @_APPLICATION_NAME_@"

<body>
        <div id="overDiv" style="position: absolute; visibility: hidden; z-index: 1000;"></div>
        <div id="rb_block1" class="rb_block">
                <table class='dataTable'>
                        <tr id="row_1" class="tableRowOdd">
                                <td>
                                        <input size="10" name="rate1" />
                                </td>
                                <td>
                                        <input size="10" name="band2" />
                                </td>
                                <td>
